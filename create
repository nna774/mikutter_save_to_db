create table mikutter.tweets(created_at varchar(50)
                            , id BIGINT UNSIGNED
                            , message varchar(350)
                            , source varchar(200)
                            , coordinates1 double
                            , coordinates2 double
                            , hashtags varchar(350)
                            , urls varchar(350)
                            , user_mentions varchar(350)
                            , favorite_count int UNSIGNED
                            , favorited int UNSIGNED
                            , in_reply_to_screen_name varchar(50)
                            , in_reply_to_status_id BIGINT UNSIGNED
                            , in_reply_to_user_id BIGINT UNSIGNED
                            , lang varchar(5)
                            , place_country varchar(50)
                            , place_country_code varchar(5)
                            , place_full_name varchar(200)
                            , place_id varchar(50)
                            , place_name varchar(50)
                            , place_type varchar(50)
                            , possibly_sensitive int UNSIGNED
                            , retweet_count int UNSIGNED
                            , retweeted int UNSIGNED
                            , user varchar(50)
                            , user_protected int UNSIGNED
                            , user_id varchar(50)
                            , user_idname varchar(50)
                            , user_name varchar(50)
                            , user_location varchar(50)
                            , user_detail varchar(400)
                            , user_url varchar(200)
                            , user_followers_count int UNSIGNED
                            , user_statuses_count int UNSIGNED
                            , user_friends_count int UNSIGNED
                            );

create table mikutter.deleted(id varchar(25),user_id varchar(25));

